@model Palmisano.EstudianteMateria

@{
    ViewData["Title"] = "Edit";
}

<h1>Editar</h1>

<h4>EstudianteMateria</h4>
<hr />
<div class="row">
    <div class="col-md-8">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="EstudianteId" />
            <input type="hidden" asp-for="MateriaId" />
            <div class="form-group">
                <label asp-for="FechaInscripcion" class="control-label"></label>
                <input asp-for="FechaInscripcion" class="form-control" />
                <span asp-validation-for="FechaInscripcion" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AñoCursado" class="control-label"></label>
                <input asp-for="AñoCursado" class="form-control" />
                <span asp-validation-for="AñoCursado" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Guardar" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Regresar a la lista</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
